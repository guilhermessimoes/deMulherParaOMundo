<%- include("./partials/header") %>
      <div class="carousel carousel-slider center">
        <div class="carousel-fixed-item center">
          <a class="btn waves-effect white grey-text darken-text-2">saiba mais</a>
        </div>
        <div class="carousel-item grey lighten-2 black-text" href="#one!">
          
          <img src="/images/modelo3.png" alt="">
        </div>
        <div class="carousel-item amber white-text" href="#two!">
          <h2>Second Panel</h2>
          <p class="white-text">This is your second panel</p>
        </div>
        <div class="carousel-item green white-text" href="#three!">
          <h2>Third Panel</h2>
          <p class="white-text">This is your third panel</p>
        </div>
        <div class="carousel-item blue white-text" href="#four!">
          <h2>Fourth Panel</h2>
          <p class="white-text">This is your fourth panel</p>
        </div>
      </div>
      <div class="container" id="cardHome">
        <blockquote>
          <h3> Conheça essas mulheres incríveis que nos inspiram todos os dias. </h3>
        </blockquote>
        <div class="row">
          <a class="btn-floating right tooltipped btn-large waves-effect waves-light deep-purple darken-4" data-position="right" data-tooltip="Cadastrar novo artigo" href="/cadastroMulher"><i class="material-icons">add</i></a>
        </div>
        <div class="row">
          <%if (messages.length > 0) { %>
            <div class="materialert success">
                <div class="material-icons">check</div>
                <%= messages %>
            </div>
          <% } %>
          <div class="col s12">
            <ul class="tabs">
              <li class="tab col s3"><a class="active purple-text" href="#todos">Todos</a></li>
              <li class="tab col s3"><a class="purple-text" href="#ciencia">Ciência</a></li>
              <li class="tab col s3"><a class="purple-text" href="#arte">Artes</a></li>
              <li class="tab col s3"><a class="purple-text" href="#social">Social</a></li>
            </ul>
          </div>
          <div id="todos" class="col s12">            
            <div class="row">
            <% for(let i=0; i < mulheres.length; i++){ %>
              <div class="col s12 m6 l4">
                <div class="card">
                  <div class="card-image">
                    <img src="/data/uploads/<%= mulheres[i].imagem %>">                
                  </div>
                  <div class="card-content">
                    <span class="card-title"><%= mulheres[i].nome %></span>
                    <p><%= mulheres[i].descricao %></p>
                  </div>
                  <div class="card-action">
                    <a class="red-text" href="">Excluir</a>
                    <a class="" href="">Editar</a>
                    <a class=" modal-trigger" href="#modal<%= i %>">Visualizar</a>
                  </div>
                </div>
                 <!-- Modal Structure -->
                <div id="modal<%= i %>" class="modal">
                  <div class="modal-content">
                    <h4><%= mulheres[i].nome %> </h4>
                    <p><%= mulheres[i].descricao %> </p>
                  </div>
                  <div class="modal-footer">
                    <a href="#!" class="modal-close waves-effect waves-green btn-flat">Fechar</a>
                  </div>
                </div>  
              </div>
            <% } %>
            </div>
          </div>
          <div id="ciencia" class="col s12">
            <div class="row">
              <% for(let i=0; i < mulheres.length; i++){ %>
                <% if(mulheres[i].genero == "ciencia"){ %>
                  <div class="col s12 m6 l4">
                    <div class="card">
                      <div class="card-image">
                        <img src= "<%= mulheres[i].imagem %>">                
                      </div>
                      <div class="card-content">
                        <span class="card-title"><%= mulheres[i].nome %></span>
                        <p><%= mulheres[i].descricao%></p>
                      </div>
                      <div class="card-action">
                        <a class="" href="">Excluir</a>
                        <a class="" href="">Editar</a>
                        <a class="modal-trigger" href="#modal<%= i %>">Saiba Mais</a>
                      </div>
                  </div>
                  </div>
                <% } %>
              <% } %>
            </div>
          </div>
          <div id="arte" class="col s12">
            <div class="row">
              <% for(let i=0; i < mulheres.length; i++){ %>
                <% if(mulheres[i].genero == "arte"){ %>
                  <div class="col s12 m6 l4">
                    <div class="card">
                      <div class="card-image ">
                        <img src=<%= mulheres[i].imagem %>>             
                      </div>
                      <div class="card-content">
                        <span class="card-title"><%= mulheres[i].nome %> </span>
                        <p><%= mulheres[i].descricao %></p>
                      </div>
                      <div class="card-action">
                        <a class="" href="">Excluir</a>
                        <a class="" href="">Editar</a>
                        <a class="modal-trigger" href="#modal<%= i %>">Saiba Mais</a>
                      </div>
                    </div>              
                  </div>
                <% } %>
              <% } %>
            </div>
          </div>
          <div id="social" class="col s12">
            <div class="row">
              <% for(let i=0; i < mulheres.length; i++){ %>
                <% if(mulheres[i].genero == "social"){ %>
                  <div class="col s12 m6 l4">
                    <div class="card">
                      <div class="card-image ">
                        <img src=<%= mulheres[i].imagem %>>             
                      </div>
                      <div class="card-content">
                        <span class="card-title"><%= mulheres[i].nome %> </span>
                        <p><%= mulheres[i].descricao %></p>
                      </div>
                      <div class="card-action">
                        <a class="" href="">Editar</a>
                        <a class="" href="">Excluir</a>
                        <a class="modal-trigger" href="#modal<%= i %>">Saiba Mais</a>
                      </div>
                    </div>              
                  </div>
                <% } %>
              <% } %>
            </div>
          </div>
        </div>
      </div>
    </main>
<%- include("./partials/footer") %>
   